<block wx:if="{{options.show}}"><view class="{{['yb-pulldown','yb-flex','yb-align-center','yb-justify-center','data-v-e89131c8',(options.row)?'yb-row':'']}}"><view class="yb-flex yb-align-center yb-justify-center data-v-e89131c8" style="{{'margin-left:'+(options.row?'-70rpx':0)+';'}}"><view class="yb-flex indicator-icon yb-align-center yb-justify-center data-v-e89131c8"><block wx:if="{{status=='ready'||status==''}}"><view class="yb-flex indicator-symbol data-v-e89131c8" style="{{'transform:'+('rotateZ('+(status=='ready'?'180deg':0)+')')+';'}}"><block wx:if="{{$slots.pulldown-symbol}}"><slot name="pulldown-symbol"></slot></block><block wx:else><list-icon vue-id="2994052c-1" name="arrow-down" size="{{50}}" color="{{options.color}}" class="data-v-e89131c8" bind:__l="__l"></list-icon></block></view></block><block wx:if="{{status=='loading'}}"><block wx:if="{{$slots.pulldown-loading}}"><slot name="pulldown-loading"></slot></block><block wx:else><list-loading vue-id="2994052c-2" visible="{{status=='loading'}}" color="{{options.color}}" class="data-v-e89131c8" bind:__l="__l"></list-loading></block></block><block wx:if="{{status=='success'}}"><block wx:if="{{$slots.pulldown-success}}"><slot name="pulldown-success"></slot></block><block wx:else><list-icon vue-id="2994052c-3" name="hook-circle" size="{{50}}" color="{{options.color}}" class="data-v-e89131c8" bind:__l="__l"></list-icon></block></block><block wx:if="{{status=='fail'}}"><block wx:if="{{$slots.pulldown-fail}}"><slot name="pulldown-fail"></slot></block><block wx:else><list-icon vue-id="2994052c-4" name="fork-circle" size="{{50}}" color="{{options.color}}" class="data-v-e89131c8" bind:__l="__l"></list-icon></block></block><block wx:if="{{status=='end'}}"><block wx:if="{{$slots.pulldown-end}}"><slot name="pulldown-end"></slot></block><block wx:else><list-icon vue-id="2994052c-5" name="hook-circle" size="{{50}}" color="{{options.color}}" class="data-v-e89131c8" bind:__l="__l"></list-icon></block></block></view></view><view class="yb-flex indicator-text yb-align-center data-v-e89131c8"><text class="refresh-text data-v-e89131c8" style="{{'color:'+(options.color)+';'}}">{{pulldownText}}</text><block wx:if="{{options.enableRefreshTime}}"><text class="refresh-time data-v-e89131c8" style="{{'color:'+(options.color)+';'}}">{{"上次更新 "+lastTime}}</text></block></view></view></block>